{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "React Boilerplate for new projects",
  "main": "index.tsx",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack-dev-server --open --watch --config webpack.config.dev.js",
    "dev:hot": "npm run dev -- --hot",
    "test": "jest",
    "format": "pretty-quick",
    "lint": "tslint --project ./",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "mock-server": "node ./src/ToDoApp/server.js",
    "todoapp": "concurrently \"npm run dev:hot\" \"npm run mock-server\""
  },
  "husky": {
    "pre-commit": "pretty-quick --staged && npm run lint && npm run test"
  },
  "author": "Youssef Meskini",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymeskini/react-boilerplate.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/ymeskini/react-boilerplate/issues"
  },
  "engines": {
    "node": "10.15.0",
    "npm": "6.8.0"
  },
  "homepage": "https://github.com/ymeskini/react-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-info": "4.1.11",
    "@storybook/addon-knobs": "4.1.11",
    "@storybook/addons": "4.1.11",
    "@storybook/react": "4.1.11",
    "@types/jest": "23.3.13",
    "@types/react": "16.7.22",
    "@types/react-dom": "16.0.11",
    "@types/react-redux": "7.0.1",
    "@types/react-router-dom": "4.3.1",
    "@types/redux-actions": "2.3.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.0.0",
    "babel-loader": "8.0.5",
    "concurrently": "4.1.0",
    "css-loader": "2.1.0",
    "cypress": "3.1.4",
    "fork-ts-checker-webpack-plugin": "0.5.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "jest": "24.0.0",
    "jest-dom": "3.0.1",
    "json-server": "0.14.2",
    "mini-css-extract-plugin": "0.5.0",
    "prettier": "1.16.1",
    "pretty-quick": "1.10.0",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "react-hot-loader": "4.6.3",
    "react-testing-library": "5.4.4",
    "storybook-addon-jsx": "6.0.0",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-react": "3.6.0",
    "typescript": "3.2.4",
    "webpack": "4.28.4",
    "webpack-bundle-analyzer": "3.0.4",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14",
